#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;

bool check(int a[ ],int n, int x){
for(int i=0;i<n;i++){
    if(x==a[i])return true;
    
} return false;
}
int main() {
    int n;cin>>n;
    int a[n];
    for(int i=0;i<n;i++){
        cin>>a[i];
    }
    int k ;cin>>k;
    for(int i=0;i<k;i++){
        int tt; cin>>tt;
        int h=tt;
        if(check(a,n,h)) cout<<"YES"<<endl;
        else cout<<"NO"<<endl;
    }
    return 0;
}
