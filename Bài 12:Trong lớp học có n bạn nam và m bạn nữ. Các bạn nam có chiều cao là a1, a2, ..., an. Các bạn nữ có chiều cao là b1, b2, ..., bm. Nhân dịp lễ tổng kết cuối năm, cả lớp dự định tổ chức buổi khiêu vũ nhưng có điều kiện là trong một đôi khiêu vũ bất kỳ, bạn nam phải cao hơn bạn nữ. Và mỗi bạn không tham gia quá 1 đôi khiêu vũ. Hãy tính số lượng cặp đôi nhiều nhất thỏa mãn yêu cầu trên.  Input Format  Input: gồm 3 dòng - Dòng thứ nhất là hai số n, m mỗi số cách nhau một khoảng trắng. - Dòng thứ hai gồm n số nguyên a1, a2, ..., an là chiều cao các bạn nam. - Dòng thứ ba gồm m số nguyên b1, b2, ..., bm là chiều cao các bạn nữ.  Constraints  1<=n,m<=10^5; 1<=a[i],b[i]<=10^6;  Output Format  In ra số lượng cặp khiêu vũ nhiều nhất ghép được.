#include <bits/stdc++.h>
using namespace std;


int main() {
       int n, m;
    cin>>n;
    cin>>m;
    int b[n];
    int c[m];
     for(int i=0;i<n;i++){
        cin>>b[i];
    }
    for(int i=0;i<m;i++){
        cin>>c[i];
    }
    int i=0,j=0;
    int dem=0;
    while(i<n&&j<m){
        if(b[i]>c[j]){
           dem++;
            i++;
            j++;
        }
        else{
            
            i++;
        }
       
    }
   
    cout<<dem;
    return 0;
}
